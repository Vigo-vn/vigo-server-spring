spring:
  datasource:
    url: #jdbc:postgresql://${PSQL_HOST}:${PSQL_PORT}/${PSQL_DATABASE}
    username: #${PSQL_USER}
    password: #${PSQL_PASSWORD}
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update  #Hibernate ddl auto (create, create-drop, validate, update)
    properties:
      hibernate:
        format_sql: true
        dialect: org.hibernate.dialect.PostgreSQLDialect
